    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "\n",
    "import time\n",
    "content = []\n",
    "count = 0\n",
    "issue = []\n",
    "\n",
    "driver = webdriver.Chrome()\n",
    "\n",
    "for url in urls:\n",
    "    driver.get('{}'.format(url))\n",
    "    # To load entire webpage\n",
    "    time.sleep(5)\n",
    "\n",
    "    if \"select-language\" in driver.current_url:\n",
    "        \n",
    "        # Block for selecting the language\n",
    "        print(\"Select language page\")\n",
    "        button = driver.find_element(By.XPATH, \"/html/body/div[3]/div/div/div/div/div[2]/ul/li[8]/a\")\n",
    "        link = button.get_attribute(\"href\")\n",
    "        driver.get(link)\n",
    "        time.sleep(1)\n",
    "\n",
    "        if driver.current_url == \"https://commission.europa.eu/documents_en\":\n",
    "        \n",
    "            #Block for searching report\n",
    "            issue.append(url)\n",
    "            count += 1\n",
    "        \n",
    "        else: \n",
    "            text = (driver.find_element(By.XPATH, \"/html/body\").text)\n",
    "    \n",
    "    else: \n",
    "        # Printing the whole body text\n",
    "        text = (driver.find_element(By.XPATH, \"/html/body\").text)\n",
    "\n",
    "        if text == '':\n",
    "            \n",
    "            try:\n",
    "                text = extract_pdf('{}'.format(url), \"url\")\n",
    "            \n",
    "            except:\n",
    "                issue.append(url)\n",
    "                count += 1\n",
    "\n",
    "\n",
    "    # text = clean(text)\n",
    "    # Should split the page into sub paragraphs, remove the cookies mentions, remove paragraphs with less than 3 words\n",
    "\n",
    "    content.append(text)\n",
    "    #print(text)\n",
    "\n",
    "driver.close()\n",
    "\n",
    "for pdf in pdfs:\n",
    "\n",
    "    content.append(extract_pdf(pdf, \"file\"))\n"
